/* BOJ 2440 별찍기 -3 */ 

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		int n = scan.nextInt();
		char x = '*';
		for(int i=1;i<=n;i++) {
			for(int y=n;y>=i;y--) {
				System.out.print('*');
			}
			System.out.println("");
		}

	}
}

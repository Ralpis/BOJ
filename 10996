/* BOJ 10996 별찍기 21 */
import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		
		for(int i=0;i<n;i++) {
			for(int x=0;x<=(double)n/2-0.5;x++) {
				System.out.print("* ");
			}
			System.out.println("");
			for(int y=0;y<n/2;y++) {
				System.out.print(" *");
			}
			System.out.println("");
		}
		
		
	}
}

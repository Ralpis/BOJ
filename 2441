/* BOJ 2441 별찍기 4 */ 

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		int n = scan.nextInt();
		
		for(int i=0;i<n;i++) {
			for(int y=0;y<i;y++) {
				System.out.print(' ');
			}
			for(int x=i;x<n;x++) {
				System.out.print('*');
			}
			System.out.println("");
		}
		
		
	}
}

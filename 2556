/* BOJ 2556 별찍기 14 */

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		scan.close();
		
		for(int i=0;i<n;i++) {
			for(int x=0;x<n;x++) {
				System.out.print('*');
			}
			System.out.println("");
		}
		
	}
}
